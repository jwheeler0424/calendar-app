{
    "presets": [
        "@babel/preset-react",
        [
            "@babel/preset-env",
            {
                "corejs": {
                    "version": "3.15.2",
                    "proposals": true
                },
                "useBuiltIns": "usage"
            }
        ]
    ],
    "plugins": [
        "@babel/plugin-proposal-class-properties",
        "@babel/plugin-proposal-object-rest-spread"
    ]
}